{
  "name": "@bkrith/multilevel",
  "version": "1.2.4",
  "description": "server/client implementation for levelDB database",
  "main": "index.js",
  "keywords": [
    "leveldb",
    "level",
    "multilevel",
    "levelup",
    "rpc",
    "stream",
    "server",
    "client"
  ],
  "scripts": {
    "start": "nodemon --watch '**/*.ts' --exec 'ts-node' index.ts",
    "test": "nodemon --watch '**/*.ts' --exec 'ts-node' test.ts",
    "build": "tsc",
    "docs": "./node_modules/.bin/typedoc --out ./docs ./src",
    "lint": "eslint src --ext ts"
  },
  "author": "Vassilis",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "15.6.0",
    "eslint": "7.27.0",
    "nodemon": "2.0.7",
    "ts-node": "9.1.1",
    "tslint": "5.20.1",
    "typedoc": "0.20.36",
    "typescript": "4.2.4",
    "typescript-eslint-parser": "21.0.2"
  },
  "dependencies": {
    "level": "7.0.0",
    "uuid-v4": "0.1.0"
  }
}
